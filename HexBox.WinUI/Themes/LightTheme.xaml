<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="AddressBrush"
                        Color="DodgerBlue" />
    <SolidColorBrush x:Key="AlternatingDataColumnTextBrush"
                        Color="#4e4e4e" />
    <SolidColorBrush x:Key="SelectionBrush"
                     Color="#737373" />
    <SolidColorBrush x:Key="SelectionTextBrush"
                     Color="White" />
    <SolidColorBrush x:Key="Background"
                     Color="Transparent" />
    <SolidColorBrush x:Key="BorderBrush"
                     Color="Blue" />
</ResourceDictionary>
